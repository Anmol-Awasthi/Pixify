{
  "expo": {
    "name": "Pixify",
    "slug": "Pixify",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Pixify-logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/Pixify-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#17153B"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#17153B"
      },
      "package": "com.anmol7.Pixify"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "12946e3e-b9f8-4ac3-87a4-53cbe0c1cc3a"
      }
    }
  }
}
